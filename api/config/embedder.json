{
  "embedder": {
    "client_class": "DashscopeClient",
    "batch_size": 10,
    "model_kwargs": {
      "model": "text-embedding-v4",
      "dimensions": 256,
      "encoding_format": "float"
    }
  },
  "retriever": {
    "top_k": 20
  },
  "text_splitter": {
    "split_by": "word",
    "chunk_size": 350,
    "chunk_overlap": 100
  }
} 